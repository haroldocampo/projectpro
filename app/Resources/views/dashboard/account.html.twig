
{% extends 'base.html.twig' %}
{% block pageheader %}Account{% endblock %}
{% block body %}
    <div class="setup-wizard-container hide page-help resizable">
        <div class="setup-wizard-header">
            <h4>HELP</h4>
            <div class="dialog-close" onclick="closeHelp()">x</div>
        </div>
        <div class="setup-wizard-content"  style="white-space: pre-line;">
            <h5 style="text-align: center;">ACCOUNT</h5>
            Here you can view/edit your account details 
        </div>
        <div class="setup-wizard-footer">
            <button class="btn btn-secondary" onclick="closeHelp();">Close</button>
        </div>
    </div>
    <div data-ng-controller="AccountController">
        <input type="hidden" name="companyId" id="companyId" value="{{ companyId }}" />
        {#<div class="row m-b-20">
            <div class="col-xs-7">
                <div class="form-inline">
                    <div class="form-group">
                        <input type="text" style="width: 200px;" class="form-control" data-ng-model="newPaymentType.name" placeholder="Type Payment Type Here">
                    </div>
                    <button class="btn btn-success" data-ng-click="addPaymentType()">Add Payment Type</button>
                </div>

            </div>
        </div>#}
        <div class="row">
            <div class="col-xs-6 col-xs-offset-3">
                <div class="card-box">

                    <!-- Change color of red to grey -->
                    <div class="p-20" style="font-size: 15px;">
                        <div class="row">
                            <div class="col-xs-6">Company Name:</div>
                            <div class="col-xs-6">
                                <span data-ng-show="!isEditing">{[ model.companyName ]}</span>
                                <span data-ng-show="isEditing"><input style="width: 180px; display: inline;" type="text" class="form-control" data-ng-model="model.companyName"/></span>
                                <i class="fa fa-edit fa-fw fa-2x" data-ng-show="!isEditing" data-ng-click="isEditing = true"></i>
                                <i class="fa fa-check fa-fw fa-2x" data-ng-show="isEditing" data-ng-click="editCompanyName()"></i>
                            </div>

                        </div>
                        <div class="row m-t-10">
                            <div class="col-xs-6">Subscription Plan:</div>
                            <div class="col-xs-6" style="text-align: left;">${[ model.subscriptionCost ]}.00/month <i data-target="#subscriptionInfoModal" data-toggle="modal" class="fa fa-edit fa-fw fa-2x"></i></div>
                        </div>
                        <div class="row m-t-10">
                            <div class="col-xs-6">Billing Info:</div>
                            <div class="col-xs-6"><button class="btn btn-secondary" data-ng-click="initBillingInfo()" style="text-align: left;">View/Edit</button></div>
                        </div>
                        <div class="row m-t-10">
                            <div class="col-xs-6">QB Desktop Integration</div>
                            <div class="col-xs-6">
                                <label class="switch">
                                    <input type="checkbox" data-ng-model="qbIntegration" data-ng-change="setIntegration()">
                                    <span class="slider round"></span>
                                </label>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>


